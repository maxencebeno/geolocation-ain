<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8"/>
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no">
    <title>{% block title %}{% endblock %}</title>
    {% stylesheets
    '../app/Resources/public/css/*' %}
    <link href='https://fonts.googleapis.com/css?family=Candal' rel='stylesheet' type='text/css'>
    <link href='https://fonts.googleapis.com/css?family=Exo' rel='stylesheet' type='text/css'>

    <link rel="stylesheet" href="{{ asset_url }}" type="text/css"/>
    {% endstylesheets %}
    <link rel="apple-touch-icon" sizes="57x57" href="{{ asset('/apple-icon-57x57.png') }}">
    <link rel="apple-touch-icon" sizes="60x60" href="{{ asset('/apple-icon-60x60.png') }}">
    <link rel="apple-touch-icon" sizes="72x72" href="{{ asset('/apple-icon-72x72.png') }}">
    <link rel="apple-touch-icon" sizes="76x76" href="{{ asset('/apple-icon-76x76.png') }}">
    <link rel="apple-touch-icon" sizes="114x114" href="{{ asset('/apple-icon-114x114.png') }}">
    <link rel="apple-touch-icon" sizes="120x120" href="{{ asset('/apple-icon-120x120.png') }}">
    <link rel="apple-touch-icon" sizes="144x144" href="{{ asset('/apple-icon-144x144.png') }}">
    <link rel="apple-touch-icon" sizes="152x152" href="{{ asset('/apple-icon-152x152.png') }}">
    <link rel="apple-touch-icon" sizes="180x180" href="{{ asset('/apple-icon-180x180.png') }}">
    <link rel="icon" type="image/png" sizes="192x192"  href="{{ asset('/android-icon-192x192.png') }}">
    <link rel="icon" type="image/png" sizes="32x32" href="{{ asset('/favicon-32x32.png') }}">
    <link rel="icon" type="image/png" sizes="96x96" href="{{ asset('/favicon-96x96.png') }}">
    <link rel="icon" type="image/png" sizes="16x16" href="{{ asset('/favicon-16x16.png') }}">

    {% block stylesheets %}{% endblock stylesheets %}

    {% javascripts '../app/Resources/public/js/jquery.min.js'
    '../app/Resources/public/js/bootstrap.min.js'
    '../app/Resources/public/js/moment.min.js'
    '../app/Resources/public/js/jquery-ui.min.js'
    '../app/Resources/public/js/jquery-ui-touch.min.js'
    '../app/Resources/public/js/bootstrap-datetimepicker.min.js'
    '../app/Resources/public/js/locale/*'
    '../app/Resources/public/js/script.js' %}
    <script type="text/javascript" src="{{ asset_url }}"></script>
    {% endjavascripts %}
</head>
<body>
{% block header %}{% include './include/header.html.twig' %}{% endblock %}
{% for type, messages in app.session.flashbag.all() %}
    {% for key, message in messages %}
        <div class="row">
            <div class="col-md-8 col-md-offset-2 alert alert-{{ type }} op_message">
                {{ message|trans }}
            </div>
        </div>
    {% endfor %}
{% endfor %}

<div id="wrapper">
    {% block body %}

    {% endblock %}

</div>
{% block footer %}{% include './include/footer.html.twig' %}{% endblock %}

{% block javascripts %}

{% endblock %}

</body>
</html>
