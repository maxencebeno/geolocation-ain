{% extends 'GeolocationAdminBundle::layout.html.twig' %}

{% block title %}
{{ parent() }} - {{ 'views.show.show'|trans({'%entity%': 'User'}, 'JordiLlonchCrudGeneratorBundle') }}
{% endblock %}

{% block page %}

<h1>User</h1>

<form class="form-horizontal">
    <fieldset>        <div class="control-group">
            <label class="control-label">Id</label>
            <div class="controls">                <span class="input-xlarge uneditable-input">{{ entity.id }}</span>            </div>
        </div>        <div class="control-group">
            <label class="control-label">Nom</label>
            <div class="controls">                <span class="input-xlarge uneditable-input">{{ entity.nom }}</span>            </div>
        </div>        <div class="control-group">
            <label class="control-label">Adresse</label>
            <div class="controls">                <span class="input-xlarge uneditable-input">{{ entity.adresse }}</span>            </div>
        </div>        <div class="control-group">
            <label class="control-label">Codepostal</label>
            <div class="controls">                <span class="input-xlarge uneditable-input">{{ entity.codePostal }}</span>            </div>
        </div>        <div class="control-group">
            <label class="control-label">Ville</label>
            <div class="controls">                <span class="input-xlarge uneditable-input">{{ entity.ville }}</span>            </div>
        </div>        <div class="control-group">
            <label class="control-label">Tel</label>
            <div class="controls">                <span class="input-xlarge uneditable-input">{{ entity.tel }}</span>            </div>
        </div>        <div class="control-group">
            <label class="control-label">Datecreation</label>
            <div class="controls">                <span class="input-xlarge uneditable-input">{{ entity.dateCreation|date('Y-m-d H:i:s') }}</span>            </div>
        </div>        <div class="control-group">
            <label class="control-label">Datecreationentreprise</label>
            <div class="controls">                <span class="input-xlarge uneditable-input">{{ entity.dateCreationEntreprise|date('Y-m-d H:i:s') }}</span>            </div>
        </div>        <div class="control-group">
            <label class="control-label">Datemodification</label>
            <div class="controls">                <span class="input-xlarge uneditable-input">{{ entity.dateModification|date('Y-m-d H:i:s') }}</span>            </div>
        </div>        <div class="control-group">
            <label class="control-label">Siret</label>
            <div class="controls">                <span class="input-xlarge uneditable-input">{{ entity.siren }}</span>            </div>
        </div>        <div class="control-group">
            <label class="control-label">Kbis</label>
            <div class="controls">                <span class="input-xlarge uneditable-input">{{ entity.kbis }}</span>            </div>
        </div>        <div class="control-group">
            <label class="control-label">Url</label>
            <div class="controls">                <span class="input-xlarge uneditable-input">{{ entity.url }}</span>            </div>
        </div>    </fieldset>
</form>

<div>
    <div class="float-left">
        <a class="btn" href="{{ path('user') }}">
            {{ 'views.recordactions.backtothelist'|trans({}, 'JordiLlonchCrudGeneratorBundle') }}
        </a>
    </div>
    <div class="float-left">
        <a class="btn btn-primary" href="{{ path('user_edit', { 'id': entity.id }) }}">
            {{ 'views.recordactions.edit'|trans({}, 'JordiLlonchCrudGeneratorBundle') }}
        </a>
    </div>
    <div class="float-left">
        <form action="{{ path('user_delete', { 'id': entity.id }) }}" method="post">
            <input type="hidden" name="_method" value="DELETE" />
            {{ form_widget(delete_form) }}
            <button class="btn btn-danger" type="submit" onclick="return confirm('{{ 'views.recordactions.confirm'|trans({}, 'JordiLlonchCrudGeneratorBundle')|escape('js') }}');">{{ 'views.recordactions.delete'|trans({}, 'JordiLlonchCrudGeneratorBundle') }}</button>
        </form>
    </div>
</div>
{% endblock %}