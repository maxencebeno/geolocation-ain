<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Geolocation\AdminBundle\Entity\User" table="user">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="nom" type="string" column="nom" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="adresse" type="text" column="adresse" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="mdp" type="string" column="mdp" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="email" type="string" column="email" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="codePostal" type="integer" column="code_postal" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="ville" type="string" column="ville" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="tel" type="string" column="tel" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="dateCreation" type="datetime" column="date_creation" nullable="false"/>
    <field name="dateModification" type="datetime" column="date_modification" nullable="false"/>
    <field name="siret" type="string" column="siret" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="kbis" type="string" column="kbis" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="url" type="text" column="url" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
